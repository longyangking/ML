Cell[CellGroupData[{

Cell["Lagrange Multiplier", "Subsubsection",
 CellChangeTimes->{{3.687511804189437*^9, 3.6875118104620857`*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"L", "(", 
    RowBox[{"\[Omega]", ",", "b", ",", "\[Alpha]"}], ")"}], "=", 
   RowBox[{
    FractionBox["1", "2"], "||", "\[Omega]", 
    SuperscriptBox["||", "2"], 
    RowBox[{"-", 
     RowBox[{
      OverscriptBox[
